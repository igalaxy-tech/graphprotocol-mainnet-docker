set -o allexport; source .env; set +o allexport; \
envsubst < graph-node-configs/config.tmpl > graph-node-configs/config.toml; \
envsubst < grafana/provisioning/datasources/graphql.tmpl > grafana/provisioning/datasources/graphql.yml

docker network create --driver bridge graph-proxy || true

docker-compose -f compose-nginx.yml up -d
docker-compose -f compose-graphnode.yml -f compose-indexer.yml -f compose-monitoring.yml up -d --build $@